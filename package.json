{
  "name": "mine-like",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev:turbo": "next dev --turbo",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "postinstall": "npm run db:generate && node -e \"const fs=require('fs');const p='node_modules/.prisma/client/default.js';if(!fs.existsSync(p))fs.writeFileSync(p,'require(\\\"ts-node/register/transpile-only\\\");module.exports=require(\\\"./client.ts\\\");');\"",
    "test": "jest"
  },
  "prisma": {
    "seed": "node --loader tsx/esm prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "next": "16.1.1",
    "next-auth": "^5.0.0-beta.30",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "ignore-loader": "^0.1.2",
    "jest": "^30.2.0",
    "null-loader": "^4.0.1",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
